{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "claude-code-containers",
	"main": "src/index.ts",
	"compatibility_date": "2025-05-23",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"containers": [
		{
			"class_name": "MyContainer",
			"image": "./Dockerfile",
			"max_instances": 10,
			"name": "claude-code-containers"
		}
	],
	"durable_objects": {
		"bindings": [
			{
				"class_name": "MyContainer",
				"name": "MY_CONTAINER"
			}
		]
	},
	"migrations": [
		{
			"new_sqlite_classes": [
				"MyContainer"
			],
			"tag": "v1"
		}
	],
	"kv_namespaces": [
		{
			"binding": "TOKEN_CACHE",
			"id": "your-prod-token-cache-id"
		},
		{
			"binding": "GITHUB_CONFIG",
			"id": "cbbd79b3c302416e812474f4416fb663"
		}
	]
}